{
  "name": "copilot-ralph-wiggum-example",
  "version": "1.0.0",
  "description": "A Todo CLI built entirely by GitHub Copilot's runSubagent orchestration using the Ralph Wiggum technique",
  "type": "module",
  "main": "dist/src/index.js",
  "bin": {
    "todo": "dist/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run test/stats-json.test.ts",
    "check:prompts": "node scripts/check-prompts.mjs"
  },
  "keywords": [
    "github-copilot",
    "runSubagent",
    "ralph-wiggum",
    "ai-orchestration",
    "todo",
    "cli",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/karais89/copilot-ralph-wiggum-example.git"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.3",
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  }
}
